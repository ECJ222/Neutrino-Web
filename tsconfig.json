{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "jsx": "react",
        "moduleResolution": "node",
        "lib": [
        "dom",
        "webworker",
        "es2017"
        ],
        "sourceMap": true,
        "typeRoots": [
        "./node_modules/@types"
        ],
        "esModuleInterop": true,
        "baseUrl": "./src",
        "allowSyntheticDefaultImports": true,
    },
    "exclude": [
        "node_modules",
        "static",
    ]
}
